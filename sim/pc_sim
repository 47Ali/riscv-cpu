    .scope S_0x132805a00;
T_1 ;
    %vpi_call 2 18 "$dumpfile", "sim/pc.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x132805a00 {0 0 0};
